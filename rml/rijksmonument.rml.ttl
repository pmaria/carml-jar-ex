######################################################################################
# Main source
######################################################################################
:CEO_rijksmonument_LS
  rml:source [
    a carml:Stream ;
    ] ;
#    carml:url "cho_instance_vb1.xml" ;
    rml:referenceFormulation ql:XPath ;
    rml:iterator "/cultuurHistorischeObjecten/rijksmonument" ;
.

######################################################################################
# rijksmonument
######################################################################################
:CEO_rijksmonument a rr:TriplesMap ;
  rml:logicalSource :CEO_rijksmonument_LS ;
  rr:subjectMap [
    rr:template "https://linkeddata.cultureelerfgoed.nl/cho-kennis/id/rijksmonument/{choId}" ;#TODO: check naam  URI
    rr:class
      ceo:Rijksmonument ;
    rr:graph ceog: ;
  ] ;
  rr:predicateObjectMap
    :CEO_choId_POM ,
    :CEO_samenhang_POM ;
.

