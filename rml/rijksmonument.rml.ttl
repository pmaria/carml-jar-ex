######################################################################################
# Main source
######################################################################################
:CEO_rijksmonument_LS
  rml:source [
      a carml:Stream ;
  ] ;
#    carml:url "cho_instance_vb1.xml" ;
  rml:referenceFormulation ql:XPath ;
  rml:iterator "/cultuurHistorischeObjecten/rijksmonument" ;
.

######################################################################################
# rijksmonument
######################################################################################
:CEO_rijksmonument a rr:TriplesMap ;
  rml:logicalSource :CEO_rijksmonument_LS ;
  rr:subjectMap [
    rr:template "https://linkeddata.cultureelerfgoed.nl/cho-kennis/id/rijksmonument/{choId}" ;
    rr:class
      ceo:Rijksmonument ;
    rr:graph ceog: ;
  ] ;
  rr:predicateObjectMap
    :CEO_choId_POM ,
    :CEO_heeftBasisregistratieRelatie_POM ,
    :CEO_heeftOmschrijving_POM ,
    :CEO_heeftOorspronkelijkeFunctie_POM ,
    :CEO_heeftHuidigeFunctie_POM ,
    :CEO_heeftGeometrie_POM ,
    :CEO_heeftLocatieAanduiding_POM ,
    :CEO_samenhang_POM ,
    [
      rr:predicate ceo:rijksmonumentnummer ;
      rr:objectMap [
        rml:reference "rijksmonumentnummer" ;
        rr:datatype xsd:integer ;
      ] ;
    ] ,
    [
      rr:predicate ceo:heeftJuridischeStatus ;
      rr:objectMap [
        rml:reference "juridischeStatus/uri" ;
        rr:termType rr:IRI ;
      ] ;
    ] ,
    [
      rr:predicate ceo:heeftMonumentAard ;
      rr:objectMap [
        rml:reference "aardMonument/uri" ;
        rr:termType rr:IRI ;
      ] ;
    ] ;
.
